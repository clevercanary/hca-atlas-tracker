{
  "name": "hca-atlas-tracker",
  "version": "1.55.0",
  "private": true,
  "scripts": {
    "dev": "./scripts/dev.sh hca-atlas-tracker && ./scripts/set-version.sh dev && next dev",
    "build:local": "./scripts/build.sh hca-atlas-tracker local && ./scripts/set-version.sh && next build",
    "build:dev": "./scripts/build.sh hca-atlas-tracker dev && ./scripts/set-version.sh && next build",
    "build:prod": "./scripts/build.sh hca-atlas-tracker prod && ./scripts/set-version.sh && next build",
    "start": "next start",
    "lint": "next lint --dir .",
    "check-format": "prettier --check .",
    "prepare": "husky install .husky",
    "test": "jest --runInBand",
    "build-catalog": "esrun ./files/build-catalog.ts",
    "migrate": "ts-node -O '{\"module\": \"commonjs\"}' ./scripts/migration-runner.ts -j ts",
    "download-data-dictionary": "esrun ./catalog/build/download-data-dictionary.ts",
    "migrate:down": "ts-node -O '{\"module\": \"commonjs\"}' ./scripts/migration-down.ts -j ts",
    "dump-schema": "pg_dump --schema-only --schema=hat --no-owner --no-privileges atlas-tracker > schema.sql"
  },
